    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@react-theming/storybook-addon": "^1.1.1",
    "@storybook/addon-actions": "^6.4.0-beta.23",
    "@storybook/addon-essentials": "^6.4.0-beta.23",
